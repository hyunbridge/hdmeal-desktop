{
  "build": {
    "productName": "흥덕고 급식",
    "appId": "kr.hdml",
    "asar": true,
    "win": {
      "target": [
        "nsis-web",
        "appx"
      ],
      "icon": "./assets/icons/win/Icon.ico"
    },
    "nsis": {
      "oneClick": true,
      "deleteAppDataOnUninstall": true,
      "language": 1042
    },
    "nsisWeb": {
      "artifactName": "HDMeal-WebSetup-${version}.${ext}"
    },
    "publish": {
      "provider": "github",
      "owner": "HDMeal",
      "repo": "HDMeal-Windows"
    },
    "appx": {
      "identityName": "11694HGSEO.40629E04BD3CC",
      "publisher": "CN=EC9A175E-F34C-4868-A7C7-E43F825E418D",
      "publisherDisplayName": "HGSEO",
      "applicationId": "kr.hdml",
      "displayName": "흥덕고 급식",
      "backgroundColor": "transparent",
      "languages": [
        "KO-KR"
      ]
    },
    "directories": {
      "buildResources": "assets",
      "output": "dist",
      "app": "."
    }
  },
  "name": "HDMeal",
  "version": "1.3.5",
  "description": "용인 흥덕고등학교 학생들을 위한 급식 앱입니다.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build": "npm run build:web && npm run build:appx",
    "build:web": "electron-builder --win nsis-web --x64 --ia32 --arm64",
    "build:appx": "electron-builder --win appx --ia32 --x64 --arm64",
    "build:test": "electron-builder --win nsis --x64"
  },
  "author": "Hyungyo Seo",
  "license": "MIT",
  "devDependencies": {
    "electron": "^8.2.3",
    "electron-builder": "^22.6.0"
  },
  "dependencies": {
    "electron-updater": "^4.3.1"
  }
}
